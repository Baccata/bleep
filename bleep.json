{
  "version" : "1",
  "scripts": {
    "native-image": "infrastructure/bleep.infra.GenNativeImage"
  },
  "platforms" : {
    "jvm" : {
      "options" : "-Duser.dir=/home/n645892/pr/bleep",
      "name" : "jvm"
    }
  },
  "scala" : {
    "2.13" : {
      "version" : "2.13.6",
      "options" : [
        "-Vtype-diffs",
        "-Vimplicits",
        "-Wvalue-discard",
        "-Wunused:privates",
        "-Wunused:patvars",
        "-Wunused:params",
        "-Wunused:locals",
        "-Wunused:imports",
        "-Wunused:explicits",
        "-Wunused:implicits",
        "-Wnumeric-widen",
        "-Wextra-implicit",
        "-Wdead-code",
        "-Wunused:nowarn",
        "-Xlint:-byname-implicit",
        "-Xlint:type-parameter-shadow",
        "-Xlint:strict-unsealed-patmat",
        "-Xlint:stars-align",
        "-Xlint:private-shadow",
        "-Xlint:poly-implicit-overload",
        "-Xlint:package-object-classes",
        "-Xlint:option-implicit",
        "-Xlint:nullary-unit",
        "-Xlint:missing-interpolator",
        "-Xlint:infer-any",
        "-Xlint:inaccessible",
        "-Xlint:doc-detached",
        "-Xlint:deprecation",
        "-Xlint:delayedinit-select",
        "-Xlint:constant",
        "-Xlint:adapted-args",
        "-Xcheckinit",
        "-unchecked",
        "-language:implicitConversions",
        "-language:higherKinds",
        "-language:experimental.macros",
        "-language:existentials",
        "-feature",
        "-explaintypes",
        "-encoding",
        "utf8"
      ]
    }
  },
  "projects" : {
    "bleep" : {
      "dependsOn" : "bleep-core",
      "sbt-scope" : "main",
      "dependencies" : "org.scalameta:svm-subs:101.0.0",
      "scala" : "2.13",
      "platform" : {
        "extends": "jvm",
        "mainClass": "bleep.Main"
      }
    },
    "bleep-core" : {
      "sbt-scope" : "main",
      "dependencies" : [
        "ch.epfl.scala::bloop-config:1.4.9",
        "io.circe::circe-generic:0.14.1",
        "io.circe::circe-parser:0.14.1",
        "io.get-coursier::coursier:2.0.16",
        "org.scala-lang:scala-reflect:2.13.6"
      ],
      "scala" : "2.13",
      "platform" : "jvm"
    },
    "bleep-tasks" : {
      "dependsOn" : "bleep-core",
      "sbt-scope" : "main",
      "scala" : "2.13",
      "platform" : "jvm"
    },
    "bleep-test" : {
      "folder" : "./bleep",
      "dependsOn" : "bleep",
      "sbt-scope" : "test",
      "dependencies" : "org.scalatest::scalatest:3.2.10",
      "scala" : "2.13",
      "platform" : "jvm"
    },
    "infrastructure" : {
      "dependsOn" : "bleep-tasks",
      "sbt-scope" : "main",
      "scala" : "2.13",
      "platform" : "jvm"
    }
  }
}